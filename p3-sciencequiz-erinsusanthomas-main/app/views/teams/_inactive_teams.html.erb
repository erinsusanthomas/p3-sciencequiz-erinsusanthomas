<br /><br /><br />
<% unless @inactive_teams.empty? %>

  <ul class="collapsible">
  <li>
    <div class="collapsible-header"> <p> <b> Inactive Teams </b> <p>
    <i class="material-icons"> expand_more </i></div>
    <div class="collapsible-body">
    <ul>
      <% for team in @inactive_teams %>
      <li><%= link_to team.name, team_path(team) %></li>
      <% end %>
    </ul>
    </div>
  </li>
  </ul>
  
<% end %>