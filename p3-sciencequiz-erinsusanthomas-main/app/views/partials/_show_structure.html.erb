<div class="row">
  <div class="col s7">
    <%= render partial: "#{primary}" %>
    <% unless secondary.nil? %>
      <p>&nbsp;</p>
      <%= render partial: "#{secondary}" %>
    <% end %>
  </div>
  <div class="col s5">
    <%= render partial: "#{sidebar1}" unless sidebar1.blank? %>
    <%= render partial: "#{sidebar2}" unless sidebar2.blank? %>
  </div>
</div>
<div class="row">
  <%= render :partial => "partials/operations_partials/object_options", locals: {model_name: model_name, object: object} %>
</div>